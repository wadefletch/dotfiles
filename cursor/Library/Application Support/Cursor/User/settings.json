  "claude-code.selectedModel": "default",
  "editor.accessibilitySupport": "off",
  "cursor.diffs.useCharacterLevelDiffs": true,
  "amp.url": "https://ampcode.com/",
  "amp.tab.enabled": true,
  "editor.wordBasedSuggestions": "off",
