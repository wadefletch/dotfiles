{
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "permissions": {
    "allow": [
      "WebFetch",
      "WebSearch",
      "Bash(node --version)",
      "Bash(pnpm --version)",
      "Bash(pnpm lint)",
      "Bash(pnpm lint:*)",
      "Bash(pnpm format)",
      "Bash(pnpm format:*)",
      "Bash(pnpm test)",
      "Bash(pnpm test:*)",
      "Bash(pnpm list)",
      "Bash(pnpm list:*)",
      "Bash(pnpm ls:*)",
      "Bash(pnpm why:*)",
      "Bash(pwd)",
      "Bash(whoami)",
      "Bash(date)",
      "Bash(curl:*)",
      "Bash(grep:*)",
      "Bash(rg:*)",
      "Bash(cat:*)",
      "Bash(cd:*)",
      "Bash(ls:*)",
      "Bash(echo:*)",
      "Bash(which:*)",
      "Bash(find:*)",
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git show:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(gh pr view:*)",
      "Bash(gh pr list:*)",
      "Bash(gh pr diff:*)",
      "Bash(gh run view:*)",
      "Bash(gh run logs:*)",
      "mcp__plugin_context7_context7"
    ],
    "deny": [
      "Edit(~/.claude/references/**)",
      "Write(~/.claude/references/**)",
      "NotebookEdit(~/.claude/references/**)"
    ],
    "additionalDirectories": [
      "~/.claude/references"
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.sh"
  },
  "enabledPlugins": {
    "code-simplifier@claude-plugins-official": true,
    "context7@claude-plugins-official": true,
    "frontend-design@claude-plugins-official": true,
    "plugin-dev@claude-plugins-official": true,
    "core@tractorbeam": true,
    "git@tractorbeam": true,
    "linear@tractorbeam": true,
    "typescript-lsp@claude-plugins-official": false,
    "rust-analyzer-lsp@claude-plugins-official": false,
    "agent-browser@agent-browser": true
  },
  "extraKnownMarketplaces": {
    "tractorbeam": {
      "source": {
        "source": "github",
        "repo": "tractorbeamai/claude"
      }
    }
  },
  "autoUpdatesChannel": "latest"
}
